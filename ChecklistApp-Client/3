export const ChecklistBody = ({ object }) => {
  async function removeActivity() {
    return fetch(`http://localhost:8080/api/activities`, {
      method: "DELETE",
      headers: {
        "Content-Type": "application/json",
      },
      body: {
        id: object.id,
      },
    }).then((data) => data.json());
  }

  return <button onClick={removeActivity}>X</button>;
};
